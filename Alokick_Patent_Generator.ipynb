    "def extract_text_from_pdf(pdf_path):\n",
    "    \"\"\"Extract all text content from the source patent PDF.\"\"\"\n",
    "    print(f\"\\nüìÑ Extracting text from: {pdf_path}\")\n",
    "    \n",
    "    doc = fitz.open(pdf_path)\n",
    "    full_text = \"\"\n",
    "    \n",
    "    for page_num in range(len(doc)):\n",
    "        page = doc[page_num]\n",
    "        text = page.get_text(\"text\")\n",
    "        full_text += text + \"\\n\\n\"\n",
    "        \n",
    "    doc.close()\n",
    "    \n",
    "    print(f\"   ‚úì Extracted {len(full_text):,} characters from {len(doc)} pages\")\n",
    "    return full_text\n",
    "\n",
    "\n",
    "def extract_figures_from_pdf(pdf_path, output_dir=\"extracted_figures\"):\n",
    "    \"\"\"Extract all images/figures from the PDF at full resolution.\"\"\"\n",
    "    print(f\"\\nüñºÔ∏è  Extracting figures from: {pdf_path}\")\n",
    "    \n",
    "    if not os.path.exists(output_dir):\n",
    "        os.makedirs(output_dir)\n",
    "    \n",
    "    doc = fitz.open(pdf_path)\n",
    "    figures = []\n",
    "    image_count = 0\n",
    "    \n",
    "    for page_num in range(len(doc)):\n",
    "        page = doc[page_num]\n",
    "        image_list = page.get_images(full=True)\n",
    "        \n",
    "        for img_index, img in enumerate(image_list):\n",
    "            xref = img[0]\n",
    "            \n",
    "            try:\n",
    "                base_image = doc.extract_image(xref)\n",
    "                image_bytes = base_image[\"image\"]\n",
    "                image_ext = base_image[\"ext\"]\n",
    "                \n",
    "                pil_image = PILImage.open(BytesIO(image_bytes))\n",
    "                \n",
    "                image_count += 1\n",
    "                image_filename = f\"{output_dir}/figure_{image_count:02d}.{image_ext}\"\n",
    "                pil_image.save(image_filename, quality=100)\n",
    "                \n",
    "                figures.append({\n",
    "                    'path': image_filename,\n",
    "                    'width': pil_image.width,\n",
    "                    'height': pil_image.height,\n",
    "                    'page': page_num + 1,\n",
    "                    'index': image_count\n",
    "                })\n",
    "                \n",
    "                print(f\"   ‚úì Figure {image_count}: {pil_image.width}x{pil_image.height}\")\n",
    "                \n",
    "            except Exception as e:\n",
    "                print(f\"   ‚ö† Could not extract image {img_index} from page {page_num + 1}: {e}\")\n",
    "    \n",
    "    # If no embedded images, render pages as images\n",
    "    if len(figures) == 0:\n",
    "        print(\"   ‚Ñπ No embedded images found. Rendering pages as figures...\")\n",
    "        for page_num in range(len(doc)):\n",
    "            page = doc[page_num]\n",
    "            \n",
    "            mat = fitz.Matrix(300/72, 300/72)  # 300 DPI\n",
    "            pix = page.get_pixmap(matrix=mat)\n",
    "            \n",
    "            image_count += 1\n",
    "            image_filename = f\"{output_dir}/figure_{image_count:02d}.png\"\n",
    "            pix.save(image_filename)\n",
    "            \n",
    "            figures.append({\n",
    "                'path': image_filename,\n",
    "                'width': pix.width,\n",
    "                'height': pix.height,\n",
    "                'page': page_num + 1,\n",
    "                'index': image_count\n",
    "            })\n",
    "            \n",
    "            print(f\"   ‚úì Page {page_num + 1} ‚Üí Figure {image_count}: {pix.width}x{pix.height}\")\n",
    "    \n",
    "    doc.close()\n",
    "    print(f\"   ‚úì Total figures: {len(figures)}\")\n",
    "    \n",
    "    return figures\n",
    "\n",
    "print(\"‚úì Extraction functions defined\")"
   ]
