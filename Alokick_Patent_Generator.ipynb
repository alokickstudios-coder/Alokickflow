    "## Step 6: Section Parser"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Section markers for parsing\n",
    "SECTION_MARKERS = [\n",
    "    (\"TITLE OF THE INVENTION\", \"title\"),\n",
    "    (\"TITLE\", \"title\"),\n",
    "    (\"ABSTRACT\", \"abstract\"),\n",
    "    (\"FIELD OF THE INVENTION\", \"field\"),\n",
    "    (\"FIELD\", \"field\"),\n",
    "    (\"BACKGROUND OF THE INVENTION\", \"background\"),\n",
    "    (\"BACKGROUND\", \"background\"),\n",
    "    (\"SUMMARY OF THE INVENTION\", \"summary\"),\n",
    "    (\"SUMMARY\", \"summary\"),\n",
    "    (\"BRIEF DESCRIPTION OF THE FIGURES\", \"brief_figures\"),\n",
    "    (\"BRIEF DESCRIPTION OF THE DRAWINGS\", \"brief_figures\"),\n",
    "    (\"BRIEF DESCRIPTION OF DRAWINGS\", \"brief_figures\"),\n",
    "    (\"DETAILED DESCRIPTION OF THE INVENTION\", \"detailed\"),\n",
    "    (\"DETAILED DESCRIPTION\", \"detailed\"),\n",
    "    (\"ENABLEMENT\", \"enablement\"),\n",
    "    (\"ENABLEMENT & TRAINING PROCEDURES\", \"enablement\"),\n",
    "    (\"TRAINING PROCEDURES\", \"training\"),\n",
    "    (\"SECURITY\", \"security\"),\n",
    "    (\"SECURITY & ANTI-ABUSE\", \"security\"),\n",
    "    (\"ANTI-ABUSE\", \"security\"),\n",
    "    (\"COPYRIGHT\", \"copyright\"),\n",
    "    (\"COPYRIGHT LEDGER\", \"copyright\"),\n",
    "    (\"COPYRIGHT AUTOMATION ENGINE\", \"copyright\"),\n",
    "    (\"ALTERNATIVE IMPLEMENTATIONS\", \"alternatives\"),\n",
    "    (\"ALTERNATIVE EMBODIMENTS\", \"alternatives\"),\n",
    "    (\"SECONDARY INVENTION\", \"secondary\"),\n",
    "    (\"CLAIMS\", \"claims\"),\n",
    "    (\"CLAIM SET\", \"claims\"),\n",
    "    (\"COMPLETE CLAIM SET\", \"claims\"),\n",
    "    (\"COMPARATIVE NOVELTY ANALYSIS\", \"novelty\"),\n",
    "    (\"NOVELTY ANALYSIS\", \"novelty\"),\n",
    "    (\"INVENTIVE STEP\", \"inventive\"),\n",
    "    (\"INDUSTRIAL APPLICABILITY\", \"industrial\"),\n",
    "    (\"BEST MODE\", \"best_mode\"),\n",
    "    (\"PCT\", \"pct\"),\n",
    "    (\"PCT/WIPO BOILERPLATE\", \"pct\"),\n",
    "    (\"WIPO\", \"pct\"),\n",
    "    (\"FIGURES\", \"figures\"),\n",
    "    (\"DRAWINGS\", \"figures\"),\n",
    "]\n",
    "\n",
    "def parse_sections(text):\n",
    "    \"\"\"Parse the extracted text into structured sections.\"\"\"\n",
    "    print(\"\\nüìã Parsing document sections...\")\n",
    "    \n",
    "    sections = {}\n",
    "    current_section = \"preamble\"\n",
    "    current_content = []\n",
    "    \n",
    "    lines = text.split('\\n')\n",
    "    \n",
    "    for line in lines:\n",
    "        stripped = line.strip().upper()\n",
    "        \n",
    "        found_section = None\n",
    "        for marker, section_key in SECTION_MARKERS:\n",
    "            if marker in stripped and len(stripped) < len(marker) + 20:\n",
    "                found_section = section_key\n",
    "                break\n",
    "        \n",
    "        if found_section:\n",
    "            if current_content:\n",
    "                if current_section not in sections:\n",
    "                    sections[current_section] = \"\"\n",
    "                sections[current_section] += '\\n'.join(current_content) + \"\\n\\n\"\n",
    "            \n",
    "            current_section = found_section\n",
    "            current_content = [line.strip()]\n",
    "        else:\n",
    "            current_content.append(line)\n",
    "    \n",
    "    if current_content:\n",
    "        if current_section not in sections:\n",
    "            sections[current_section] = \"\"\n",
    "        sections[current_section] += '\\n'.join(current_content)\n",
    "    \n",
    "    for key, content in sections.items():\n",
    "        print(f\"   ‚úì {key}: {len(content):,} chars\")\n",
    "    \n",
    "    return sections\n",
    "\n",
    "\n",
    "def parse_claims(claims_text):\n",
    "    \"\"\"Parse claims into individual numbered claims with proper structure.\"\"\"\n",
    "    claims = []\n",
    "    \n",
    "    claim_pattern = re.compile(\n",
    "        r'(?:^|\\n)\\s*(?:Claim\\s+)?(\\d+)[.):]\\s*',\n",
    "        re.IGNORECASE\n",
    "    )\n",
    "    \n",
    "    parts = claim_pattern.split(claims_text)\n",
    "    \n",
    "    i = 1\n",
    "    while i < len(parts):\n",
    "        claim_num = parts[i]\n",
    "        if i + 1 < len(parts):\n",
    "            claim_text = parts[i + 1].strip()\n",
    "            if claim_text:\n",
    "                is_dependent = bool(re.search(\n",
    "                    r'(?:according to|as claimed in|of claim|per claim)\\s+(?:claim\\s+)?\\d+',\n",
    "                    claim_text,\n",
    "                    re.IGNORECASE\n",
    "                ))\n",
    "                \n",
    "                claims.append({\n",
    "                    'number': int(claim_num),\n",
    "                    'text': claim_text,\n",
    "                    'is_dependent': is_dependent\n",
    "                })\n",
    "        i += 2\n",
    "    \n",
    "    claims.sort(key=lambda x: x['number'])\n",
    "    \n",
    "    print(f\"   ‚úì Parsed {len(claims)} claims\")\n",
    "    return claims\n",
    "\n",
    "print(\"‚úì Parser functions defined\")"
   ]
