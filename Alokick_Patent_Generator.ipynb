    "def clean_paragraph(text):\n",
    "    \"\"\"Clean and format a paragraph for PDF output.\"\"\"\n",
    "    text = re.sub(r'\\s+', ' ', text)\n",
    "    text = text.strip()\n",
    "    \n",
    "    # Escape XML characters\n",
    "    text = text.replace('&', '&amp;')\n",
    "    text = text.replace('<', '&lt;')\n",
    "    text = text.replace('>', '&gt;')\n",
    "    \n",
    "    # Convert symbols\n",
    "    replacements = {\n",
    "        '‚Äì': '-', '‚Äî': '-', '"': '\"', '"': '\"',\n",
    "        ''': \"'\", ''': \"'\", '‚Ä¶': '...',\n",
    "        '√ó': 'x', '‚Üí': '-&gt;', '‚Üê': '&lt;-',\n",
    "        '‚â•': '&gt;=', '‚â§': '&lt;=', '‚â†': '!=',\n",
    "        '‚àû': 'infinity', 'œÄ': 'pi',\n",
    "        '‚àë': 'SUM', '‚àè': 'PRODUCT', '‚àö': 'sqrt',\n",
    "        'Œ±': 'alpha', 'Œ≤': 'beta', 'Œ≥': 'gamma',\n",
    "        'Œ¥': 'delta', 'Œµ': 'epsilon', 'Œ∏': 'theta',\n",
    "        'Œª': 'lambda', 'Œº': 'mu', 'œÉ': 'sigma', 'œâ': 'omega',\n",
    "    }\n",
    "    \n",
    "    for old, new in replacements.items():\n",
    "        text = text.replace(old, new)\n",
    "    \n",
    "    return text\n",
    "\n",
    "\n",
    "def format_section_content(content, styles):\n",
    "    \"\"\"Convert section content into ReportLab flowables.\"\"\"\n",
    "    flowables = []\n",
    "    \n",
    "    paragraphs = content.split('\\n\\n')\n",
    "    \n",
    "    for para in paragraphs:\n",
    "        para = para.strip()\n",
    "        if not para:\n",
    "            continue\n",
    "        \n",
    "        if '```' in para or para.count('\\n') > 3:\n",
    "            cleaned = clean_paragraph(para.replace('```', ''))\n",
    "            if cleaned:\n",
    "                flowables.append(Paragraph(cleaned, styles['CodeBlock']))\n",
    "        elif re.match(r'^[A-Z][A-Z\\s]+:?\\s*$', para):\n",
    "            cleaned = clean_paragraph(para)\n",
    "            if cleaned:\n",
    "                flowables.append(Paragraph(cleaned, styles['SubsectionHeading']))\n",
    "        elif re.match(r'^\\d+\\.\\d+', para):\n",
    "            cleaned = clean_paragraph(para)\n",
    "            if cleaned:\n",
    "                flowables.append(Paragraph(cleaned, styles['SubsectionHeading']))\n",
    "        else:\n",
    "            cleaned = clean_paragraph(para)\n",
    "            if cleaned and len(cleaned) > 2:\n",
    "                flowables.append(Paragraph(cleaned, styles['PatentBody']))\n",
    "    \n",
    "    return flowables\n",
    "\n",
    "print(\"‚úì Cleaning functions defined\")"
   ]
